## Shortcuts
alias E='cd ~/Desktop'
alias D='cd ~/Downloads'
alias dotfiles='cd ~/dotfiles'
alias work='cd ~/work'
alias dev='cd ~/dev'
alias bin='cd ~/.bin'

alias ll='ls -lh'
alias rm='rm -I'
alias rmf='rm -rfI'

## je
alias :q='exit'
alias :Q='exit'
alias EXIT='exit'

## vi -> vim
alias vi='vim'

# reload configs
alias reload_tmux='tmux source-file ~/.tmux.conf'
alias reload_shell='source ~/.profile'
